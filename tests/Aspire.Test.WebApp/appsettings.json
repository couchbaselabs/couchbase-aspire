{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Aspire": {
        "Couchbase": {
            "Client": {
                "HealthChecks": {
                    "Type": "Active",
                    "MinimumHealthyNodes": {
                        "Query": 1
                    }
                }
            }
        }
    },
    "AllowedHosts": "*"
}
